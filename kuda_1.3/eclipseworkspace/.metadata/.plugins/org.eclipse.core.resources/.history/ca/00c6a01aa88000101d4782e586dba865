<script type="text/javascript" >
	
	jQuery(window).load(function() {
		

		unit1.start(onUnit1Complete);

		
	});

	function onUnit1Complete() {
		//hemi.core.client.cleanup();
		
		unit1.cleanup();
		unit2.start(onUnit2Complete);
	}
	
	function onUnit2Complete() {
		unit2.cleanup();
		unit3.start(onUnit2Complete);
	}
	
	function onUnit3Complete() {
		
		hemi.core.client.cleanup();
	}



	jQuery(window).unload(function() {
		if (hemi.core.client) {
			hemi.core.client.cleanup();
		}
	});
	
</script>


	
	
	
	
