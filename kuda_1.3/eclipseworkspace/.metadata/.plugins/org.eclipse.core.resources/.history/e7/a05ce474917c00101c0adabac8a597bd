

/**
 * @namespace A module for managing the 2D GUI
 */
var lgb = (function(lgb) {



	lgb.model = lgb.model || {};
	
	lgb.model.UserAction = function(name, type){
		
		if (name) {
			this.name = name;
		} else {
			this.name = '{no name set}';
		}
		
		if (type) {
			this.type = type;
		} else {
			this.type = this.BUTTON;
		}

	};
	
	
	lgb.model.UserAction.prototype = {
		
		init: function(){

		}
	};
	
	
		var UserActionType = (function() {
		     var private = {
		         'BUTTON': '1',
		         'TOGGLE_BUTTON': '2',
				 'SLIDER' : 3
		     };
		
		     return {
		        get: function(name) { return private[name]; }
		    };
		})();




	return lgb;
	
})(lgb || {});



