
/**
 * @namespace 
 */
var lgb = (function(lgb) {


	lgb.model = lgb.model || {};
	lgb.model.control = lgb.model.controls || {};
	
	lgb.model.control.ControlBase = function() {
		this.baseClass = 'lgb.model.control.ControlBase';
		this.title = null;
	};

	lgb.model.control.ModelBase.prototype = {
		
		
		generateId : function () {
		
			if (null === this.title) {
				throw new Error('lgb.model.control.ModelBase.generateId() title is null');	
			}
			
			
			var id = this.title.split(' ').join('-');
			
		},
		
		validateId : function (id) {
		
			if (null === id) {
				throw new Error('lgb.model.control.ModelBase.validateId() id is null');	
			}
			
			
			var id = this.title.split(' ').join('-');
			
		}
		
/*
		dispatch: function(eventName) {
			var newEvent = jQuery.Event(eventName);
			$('body').trigger(newEvent);
		},
		
		listen: function(eventName, func) {
			
			var delegate = jQuery.proxy( func, this );
			$('body').bind(eventName, delegate);
		}
*/
		
	
		
	};

	return lgb;
	
})(lgb || {});
