

var lgb = (function(lgb) {

	lgb.controller = lgb.controller || {};
	lgb.controller.control = lgb.controller.control || {};
	
	
	/**
	 * @class MVC controller for the RadioButtonGroupController
	 * @extends lgb.controller.ControllerBase
	 */
	lgb.controller.control.RadioButtonGroupController = function(dataModel) {
		
		lgb.controller.ControllerBase.call(this);
		this.init_();
		
		//dataModel.assertType(lgb.model.ModelBase);
		this.dataModel = dataModel;
		this.init_();

	};
	
	
	lgb.controller.control.RadioButtonGroupController.prototype = {
		
		init_: function() {

			this.view =  new lgb.view.control.RadioButtonGroup();
			
			//this.adminPanel.showAll(this.dataModelArray);
			//
			//this.adminPanel.injectHtml();
		
		//	lgb.model.ModelBase
			
			//this.listen(this.dataModel.name, this.onGuiEvent);
			//this.view.init(this.dataModel);
			
		},

		
		bindEvents : function() {

			this.view.bindEvents($.proxy(this.onChange, this));
			
		},
		
		onChange : function(event) {

			this.view.bindEvents();
			
		}



	
	};
	
	lgb.controller.control.RadioButtonGroupController.inheritsFrom(lgb.controller.ControllerBase);

	return lgb;
	
})(lgb || {});










