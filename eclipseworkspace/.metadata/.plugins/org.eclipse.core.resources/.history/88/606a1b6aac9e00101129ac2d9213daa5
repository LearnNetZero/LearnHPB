
/**
 * @namespace 
 */
var lgb = (function(lgb) {



	lgb.util = lgb.util || {};
	
	/**
	 * @class 
	 */
	lgb.util.Loader = function() {
		lgb.Base.call(this);
		
	};


	lgb.util.Loader.prototype = {
		
		setFloorHeight: function(floorHeight) {
			this.floorHeight = floorHeight;
			this.dispatch(lgb.event.EnvelopeEvent.DATA_MODEL_CHANGED);
		},
		setTotalFloors: function(totalFloors) {
			this.totalFloors = totalFloors;
			this.dispatch(lgb.event.EnvelopeEvent.DATA_MODEL_CHANGED);
		}
		
	};
	

	lgb.util.EnvelopeModel.inheritsFrom(lgb.Base);
	
	
	return lgb;
	
})(lgb || {});












