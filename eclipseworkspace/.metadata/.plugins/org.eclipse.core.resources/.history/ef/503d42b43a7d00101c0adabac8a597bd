o3djs.require('lgb.model.UserAction');

/**
 * @namespace A module for managing the 2D GUI
 */
var lgb = (function(lgb) {


	/**
	 * @namespace A module for managing the 2D GUI
	 */
	lgb.view = lgb.view || {};
	lgb.view.controls = lgb.view.controls || {};


	lgb.view.controls.Button = function(userAction){
		this.userAction = userAction;
	};
	
	lgb.view.controls.Button.prototype = {
	

		
		getHTML : function() {
			
			var subpanel = new lgb.view.AdminSubpanel(model);
			$(subpanel).bind("ADMIN_PANEL",clickHandler);
			
			this.subPanels.push(subpanel);

		}


	};

	
	return lgb;
	
})(lgb || {});











